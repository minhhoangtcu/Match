<AnchorPane prefHeight="25.0" prefWidth="517.0" stylesheets="@../MatchTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.match.gui.bottom.MainButtonBarController">
